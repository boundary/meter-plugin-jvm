{
  "maps": [
    {
      "mbean": "java.nio:name=direct,type=BufferPool",
      "attributes": [
        {
          "attribute": "Count",
          "dataType": "long",
          "metricName": "JAVA.NIO.BUFFERPOOL.DIRECT.COUNT",
          "metricType": "standard",
          "scale": 1,
          "enabled": true
        },
        {
          "attribute": "TotalCapacity",
          "dataType": "long",
          "metricName": "JAVA.NIO.BUFFERPOOL.DIRECT.TOTAL.CAPACITY",
          "metricType": "standard",
          "scale": 1,
          "enabled": true
        },
        {
          "attribute": "MemoryUsed",
          "dataType": "long",
          "metricName": "JAVA.NIO.BUFFERPOOL.DIRECT.MEMORY.USED",
          "metricType": "standard",
          "scale": 1,
          "enabled": true
        }
      ],
      "enabled": true
    }
  ]
}
